namespace Runtime.WordPress.Admin;

use Runtime.Web.BaseRoute;
use Runtime.Web.RouteInfo;


class Routes extends BaseRoute
{
	/**
	 * Returns layout name
	 */
	string getLayoutName() => "default";
    
    
    /**
	 * Returns routes
	 */
	pure Collection<RouteInfo> getRoutes()
	{
		return
		[
			new RouteInfo
			{
				"uri": "admin.php?page=elberos-forms",
				"name": "admin:forms:index",
				"model": "Runtime.WordPress.Admin.Forms.FormsPageModel",
			},
			new RouteInfo
			{
				"uri": "admin.php?page=elberos-forms&action=edit",
				"name": "admin:forms:edit",
				"model": "Runtime.WordPress.Admin.Forms.FormsPageModel",
			},
		];
	}
}