{
	"name": "Runtime.WordPress",
	"assets":
	[
		"Theme/Components/Form.bay",
		"Theme/Components/FormDialog.bay",
		"Theme/Components/FormDialogModel.bay",
		"Theme/Components/FormModel.bay",
		"Theme/AssetHook.bay",
		"Theme/ModuleDescription.bay",
		"WP_App.bay"
	],
	"modules":
	[
		{
			"src": "bay/Admin",
			"type": "module"
		},
		{
			"src": "bay/Theme/WidgetSettings",
			"type": "module"
		}
	],
	"src": "./bay",
	"dest":
	{
		"php": "./php",
		"es6": "./es6"
	},
	"allow":
	[
		"\\.bay$",
		"\\.php$",
		"\\.js$"
	],
	"exclude":
	[
		"Admin/",
		"Theme/WidgetSettings/"
	]
}