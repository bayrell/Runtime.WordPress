{
	"name": "Runtime.WordPress",
	"assets":
	[
		"WP_App.bay"
	],
	"modules":
	[
		{
			"src": "bay/Admin",
			"type": "module"
		},
		{
			"src": "bay/Theme",
			"type": "module"
		}
	],
	"src": "./bay",
	"dest":
	{
		"php": "./php",
		"es6": "./es6"
	},
	"allow":
	[
		"\\.bay$",
		"\\.php$",
		"\\.js$"
	],
	"exclude":
	[
		"Admin/",
		"Theme/"
	]
}